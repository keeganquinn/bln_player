{
  "name": "bln_player",
  "version": "0.5.3",
  "description": "Play music published by basslin.es records",
  "homepage": "https://github.com/keeganquinn/bln_player",
  "bugs": "https://github.com/keeganquinn/bln_player/issues",
  "license": "MIT",
  "author": "Keegan Quinn <keegan@qtk.io>",
  "repository": "https://github.com/keeganquinn/bln_player.git",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:css": "sass ./src/style.scss ./dist/style.css --no-source-map --load-path=node_modules",
    "clean": "rimraf *.xml coverage-js dist jsdoc",
    "lint": "eslint src *.js",
    "jsdoc": "jsdoc -r src -d jsdoc",
    "pkglint": "npmPkgJsonLint .",
    "prepack": "npm run build && npm run build:css",
    "server": "ws -d dist -f dev",
    "test": "jest --coverage"
  },
  "files": [
    "dist/main.js",
    "src/bln_player.js",
    "src/music_control.js",
    "src/panel_control.js"
  ],
  "keywords": [
    "basslin.es",
    "music",
    "playlist",
    "record label"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.9.5",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@popperjs/core": "^2.9.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-macros": "^3.1.0",
    "bootstrap": "^5.0.1",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-formatter-checkstyle-relative-paths": "^1.0.1",
    "eslint-plugin-import": "^2.20.2",
    "howler": "^2.1.3",
    "html-webpack-plugin": "^5.3.1",
    "html-webpack-tags-plugin": "^3.0.2",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-junit": "^14.0.0",
    "js-cookie": "^3.0.1",
    "jsdoc": "^3.6.4",
    "local-web-server": "^5.1.1",
    "loglevel": "^1.6.7",
    "mini-css-extract-plugin": "^2.4.6",
    "nouislider": "^15.1.0",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "spin.js": "^4.1.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  }
}
