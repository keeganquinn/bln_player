{
  "name": "bln_player",
  "version": "0.6.0",
  "description": "Play music published by basslin.es records",
  "homepage": "https://github.com/keeganquinn/bln_player",
  "bugs": "https://github.com/keeganquinn/bln_player/issues",
  "license": "MIT",
  "author": "Keegan Quinn <keegan@qtk.io>",
  "repository": "https://github.com/keeganquinn/bln_player.git",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:css": "sass ./src/style.scss ./dist/style.css --no-source-map --load-path=node_modules",
    "build:html": "ncp ./src/index.html ./dist/index.html",
    "clean": "rimraf *.xml coverage-js dist jsdoc",
    "lint": "eslint src",
    "doc": "jsdoc -c .jsdoc.json",
    "prepack": "npm run build && npm run build:css && npm run build:html",
    "server": "ws -d dist -f dev",
    "test": "jest --coverage"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/bln_player.js",
    "dist/bln_player.d.ts",
    "dist/music_control.js",
    "dist/music_control.d.ts",
    "dist/panel_control.js",
    "dist/panel_control.d.ts"
  ],
  "keywords": [
    "basslin.es",
    "music",
    "playlist",
    "record label"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.18.6",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@popperjs/core": "^2.9.2",
    "@types/babel__preset-env": "^7.9.2",
    "@types/howler": "^2.2.7",
    "@types/jest": "^28.1.6",
    "@types/js-cookie": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-jest": "^28.1.3",
    "bootstrap": "^5.0.1",
    "eslint": "^8.21.0",
    "howler": "^2.1.3",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-junit": "^14.0.0",
    "js-cookie": "^3.0.1",
    "jsdoc": "^3.6.4",
    "jsdoc-plugin-typescript": "^2.1.1",
    "local-web-server": "^5.1.1",
    "loglevel": "^1.6.7",
    "ncp": "^2.0.0",
    "nouislider": "^15.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "spin.js": "^4.1.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
